/****ztemt add for MHL spi mode ****/
 spi@f9966000 {
         sii8620@0{
                   status = "ok";
                   compatible = "sii8620drv";
                   reg = <0>;
                   interrupt-parent = <&msm_gpio>;
                   interrupts = <64 0x3>;
                   pinctrl-names = "sii8620_int_active","sii8620_int_suspend";
