                   sii8620-wake-gpio = <&msm_gpio 12 0x00>;
                   vdd_power-supply = <&pm8994_l25>;
                   spi-max-frequency = <35000000>;
                   qcom,hdmi-tx-map = <&mdss_hdmi_tx>;
                };
    };  

